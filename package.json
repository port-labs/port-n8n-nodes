{
	"name": "@port-labs/n8n-nodes-portio-experimental",
	"version": "1.0.0-alpha.3",
	"description": "EXPERIMENTAL: n8n nodes for Port.io",
	"license": "MIT",
	"homepage": "https://www.port.io/",
	"keywords": [
		"n8n-community-node-package",
		"port.io",
		"port",
		"ai",
		"agent"
	],
	"author": {
		"name": "Port"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/port-labs/port-n8n-nodes.git"
	},
	"scripts": {
		"build": "n8n-node build && node ./scripts/pre-release.js",
		"build:watch": "tsc --watch",
		"dev": "N8N_PORT=5680 N8N_SECURE_COOKIE=false n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"prerelease": "node ./scripts/pre-release.js"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/PortApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/PortApiAI/PortApiAi.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"rimraf": "^6.1.0",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}